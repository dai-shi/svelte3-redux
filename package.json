{
  "name": "svelte3-redux",
  "description": "Redux for Svelte 3",
  "version": "0.2.1",
  "author": "Daishi Kato",
  "repository": {
    "type": "git",
    "url": "https://github.com/dai-shi/svelte3-redux.git"
  },
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "browser": "./dist/index.umd.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "compile": "microbundle build --format cjs,es,umd",
    "test": "npm run eslint && npm run tsc-test && npm run jest",
    "eslint": "eslint --ext .js,.ts,.svelte --ignore-pattern dist .",
    "jest": "jest",
    "tsc-test": "tsc --project . --noEmit",
    "apidoc": "documentation build --format md --markdown-toc false --parse-extension ts src/*.ts | add-text-to-markdown README.md --section API --write",
    "examples:minimal": "DIR=01_minimal EXT=js webpack-dev-server",
    "examples:typescript": "DIR=02_typescript webpack-dev-server",
    "examples:worker": "DIR=03_worker webpack-dev-server"
  },
  "keywords": [
    "svelte",
    "redux",
    "store",
    "state"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "add-text-to-markdown": "^2.0.0",
    "documentation": "^12.1.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "microbundle": "^0.12.0-next.6",
    "redux": "^4.0.4",
    "svelte": "^3.12.1",
    "svelte-loader": "^2.13.6",
    "ts-loader": "^6.1.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  },
  "peerDependencies": {
    "svelte": ">=3.0.0",
    "redux": ">=4.0.0"
  }
}
